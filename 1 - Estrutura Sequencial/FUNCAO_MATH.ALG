Algoritmo "Funcao_Math"

Var
    f, g, h, a, b, c, d : real

Inicio

    // Funções Matemática no VisualG
    f <- 2.0
    g <- 3.0
    h <- -7.0
    
    a <- Exp(f, g) // Exponenciação
    b <- RaizQ(g)  // Raiz quadrada
    c <- Pi        // Pega o valor PI
    d <- Abs(h)    // Valor absoluto

    escreval(a)
    escreval(b)
    escreval(c)
    escreval(d)
Fimalgoritmo